{
  "name": "ng-extract-i18n-merge-json",
  "version": "1.0.0",
  "description": "Extract and merge i18n JSON translation files for angular projects.",
  "main": "extract-i18n/index.js",
  "builders": "builders.json",
  "schematics": "./schematics/collection.json",
  "ng-add": {
    "save": "devDependencies"
  },
  "ng-update": {
    "migrations": "./schematics/migrations/migrations.json"
  },
  "scripts": {
    "build": "tsc",
    "test": "ts-node node_modules/jasmine/bin/jasmine",
    "lint": "eslint . --ext .ts",
    "lint-fix": "eslint . --ext .ts --fix"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/karsa-mistmere/ng-extract-i18n-merge-json.git"
  },
  "keywords": [
    "angular",
    "i18n",
    "json"
  ],
  "author": "Karsa Rig√≥",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/karsa-mistmere/ng-extract-i18n-merge-json/issues"
  },
  "homepage": "https://github.com/karsa-mistmere/ng-extract-i18n-merge-json#readme",
  "dependencies": {
    "@angular-devkit/architect": "^0.1301.0 || ^0.1401.0 || ^0.1501.0",
    "@angular-devkit/core": "^13.0.0 || ^14.0.0 || ^15.0.0",
    "@angular-devkit/schematics": "^13.0.0 || ^14.0.0 || ^15.0.0",
    "@schematics/angular": "^13.0.0 || ^14.0.0 || ^15.0.0",
    "@types/jasmine": "^4.3.1",
    "@types/node": "^18.15.10",
    "jasmine": "^4.6.0",
    "jasmine-node": "^1.16.0",
    "karma-coverage": "^2.2.0"
  },
  "peerDependencies": {
    "@angular-devkit/build-angular": "^13.0.0 || ^14.0.0 || ^15.0.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "eslint": "^8.36.0",
    "ts-node": "~10.9.1",
    "typescript": "^4.9.5"
  }
}
